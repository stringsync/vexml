{
  "name": "@stringsync/vexml",
  "version": "0.0.0",
  "private": false,
  "description": "MusicXML to Vexflow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "vexflow",
    "musicxml"
  ],
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "typecheck": "yarn tsc --noEmit",
    "build:image": "docker build . --tag vexml:latest --quiet",
    "build:image:m1": "docker build . --tag vexml:latest --quiet --platform linux/amd64",
    "pretest": "yarn build:image",
    "pretest:m1": "yarn build:image:m1",
    "test": "docker run -it -v ./src:/vexml/src -v ./tests:/vexml/tests vexml:latest yarn jest --runInBand",
    "test:m1": "docker run -it -v ./src:/vexml/src -v ./tests:/vexml/tests vexml:latest yarn jest --runInBand"
  },
  "dependencies": {
    "vexflow": "^4.2.0-beta.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@prettier/plugin-xml": "^2.0.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "babel-jest": "29.6.4",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "29.6.4",
    "jest-diff": "^27.5.1",
    "jest-environment-jsdom": "29.6.4",
    "jest-extended": "^2.0.0",
    "jest-image-snapshot": "^6.2.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "puppeteer": "21.1.0",
    "react": "^18.0.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.7.4",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  }
}
